/*
How many seconds are in 30 days? 
use the info below to figure it out:

1 day = 24 hours
1 hour = 60 minutes
1 minute = 60 seconds 
*/

//code should output 2592000
const hoursInaMonth = (30*24);
const minutesInaMonth =(hoursInaMonth * 60);
const secondsInaMonth = (minutesInaMonth * 60);
console.log (secondsInaMonth);
